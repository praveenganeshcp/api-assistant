{
  "name": "projects-be",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/projects-be/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/projects-be",
        "tsConfig": "libs/projects-be/tsconfig.lib.json",
        "packageJson": "libs/projects-be/package.json",
        "main": "libs/projects-be/src/index.ts",
        "assets": ["libs/projects-be/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/projects-be/**/*.ts",
          "libs/projects-be/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/projects-be/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
