{
  "name": "application-cloud-code-be",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/application-cloud-code-be/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/application-cloud-code-be",
        "tsConfig": "libs/application-cloud-code-be/tsconfig.lib.json",
        "packageJson": "libs/application-cloud-code-be/package.json",
        "main": "libs/application-cloud-code-be/src/index.ts",
        "assets": ["libs/application-cloud-code-be/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/application-cloud-code-be/**/*.ts",
          "libs/application-cloud-code-be/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/application-cloud-code-be/jest.config.ts"
      }
    }
  }
}
