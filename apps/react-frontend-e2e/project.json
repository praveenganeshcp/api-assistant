{
  "name": "react-frontend-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/react-frontend-e2e/src",
  "implicitDependencies": ["react-frontend"],
  "tags": [],
  "// targets": "to see all targets run: nx show project react-frontend-e2e --web",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/react-frontend-e2e/cypress.config.ts",
        "testingType": "e2e",
        "baseUrl": "http://localhost:4200"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/react-frontend-e2e/**/*.{js,ts}"]
      }
    }
  }
}
